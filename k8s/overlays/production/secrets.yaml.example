apiVersion: v1
kind: Secret
metadata:
  name: cloudflared-secrets
  namespace: rag-demo
type: Opaque
stringData:
  tunnel-token: "YOUR_TUNNEL_TOKEN_HERE"
---
apiVersion: v1
kind: Secret
metadata:
  name: mcpo-secrets
  namespace: rag-demo
type: Opaque
stringData:
  github-token: "YOUR_GITHUB_TOKEN_HERE"
