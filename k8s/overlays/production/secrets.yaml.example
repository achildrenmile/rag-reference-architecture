# Copy this to secrets.yaml and fill in real values
# DO NOT commit secrets.yaml to git!
apiVersion: v1
kind: Secret
metadata:
  name: openwebui-secrets
  namespace: rag-demo
type: Opaque
stringData:
  secret-key: "YOUR_WEBUI_SECRET_KEY_HERE"
